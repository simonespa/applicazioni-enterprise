<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	>

	<bean id="user" class="ae.spring.bean.four.User" autowire="byName">
		<property name="firstname" value="Mickey" />
		<property name="lastname" value="Mouse" />
	</bean>
	<bean id="userAccount" class="ae.spring.bean.four.UserAccount" >
		<property name="name" value="mick" />
	</bean>
</beans>
