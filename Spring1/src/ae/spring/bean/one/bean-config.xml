<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="pippo" class="ae.spring.bean.one.User" />

	<bean id="pluto" class="ae.spring.bean.one.User" >
		<property name="firstname" value="Ciccio"></property>
		<property name="lastname" value="Pasticcio"></property>
	</bean>
	
	<bean id="topolino" class="ae.spring.bean.one.User" >
		<constructor-arg value="Tizio"></constructor-arg>
		<constructor-arg value="Caio"></constructor-arg>
	</bean>
	
</beans>
